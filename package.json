{
    "name": "shaanxi-law",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "install:main": "cd main && yarn",
        "dev:main": "cd main && yarn dev",
        "build:main": "cd main && yarn build",
        "install:sub": "cd sub && yarn",
        "dev:sub": "cd sub && yarn serve",
        "build:sub": "cd sub && yarn build",
        "install-all": "npm-run-all install:*",
        "dev-all": "npm-run-all --parallel dev:*",
        "build-all": "npm-run-all --parallel build:*"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    },
    "dependencies": {
        "shaanxi-law": "file:",
        "vite-plugin-qiankun": "^1.0.15"
    }
}
